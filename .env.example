# Server
NODE_ENV=development
PORT=3001

# AWS
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_key
AWS_SECRET_ACCESS_KEY=your_secret

# S3
S3_BUCKET_NAME=auto-rfp-documents
S3_KNOWLEDGE_BASE_BUCKET=auto-rfp-knowledge-base

# DynamoDB
DYNAMODB_PROPOSALS_TABLE=auto-rfp-proposals
DYNAMODB_USERS_TABLE=auto-rfp-users
DYNAMODB_KNOWLEDGE_TABLE=auto-rfp-knowledge
DYNAMODB_COMPANIES_TABLE=auto-rfp-companies
DYNAMODB_INVOICES_TABLE=auto-rfp-invoices
DYNAMODB_USAGE_RECORDS_TABLE=auto-rfp-usage-records
DYNAMODB_PAYMENT_METHODS_TABLE=auto-rfp-payment-methods

# OpenSearch
OPENSEARCH_ENDPOINT=https://your-domain.es.amazonaws.com

# Bedrock
BEDROCK_MODEL_ID=anthropic.claude-3-sonnet-20240229-v1:0

# JWT
JWT_SECRET=your-secret-key
JWT_EXPIRES_IN=7d

# CORS
ALLOWED_ORIGINS=http://localhost:3000